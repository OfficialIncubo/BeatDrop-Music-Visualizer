warp_4=`    //https://www.shadertoy.com/view/3cGyRc
warp_5=`    //https://x.com/clippecac
warp_6=`    //Ported to MilkDrop by MilkDrop2077
warp_7=`
warp_8=`    uv = uv_orig * 2 - 1.0; //Center?
warp_9=`    uv = -uv; //Needs flipping first
warp_10=`    uv.x *= aspect.x / aspect.y;
warp_11=`
warp_12=`    float3 r = normalize(float3(uv, -1.0));
warp_13=`    float3 c = float3(0,0,0);
warp_14=`
warp_15=`    float t = 0.0;
warp_16=`
warp_17=`    for (float i = 0.0; i < 75.0; i += 1.0)
warp_18=`    {
warp_19=`        float3 p = r * t;
warp_20=`        p.z -= time * 3;
warp_21=`
warp_22=`        // rotation
warp_23=`        float angle = t * 0.09;
warp_24=`        float s = sin(angle), co = cos(angle);
warp_25=`        float2 px = p.yx; //Transposed to look like original
warp_26=`        p.x = px.x*co + px.y*s;
warp_27=`        p.y = px.x*s - px.y*co;
warp_28=`
warp_29=`        // L-wall
warp_30=`        float v = 6.0 * q3 - max(p.x, p.y);
warp_31=`
warp_32=`        // color accumulate
warp_33=`        c += (1.0 + cos(i*0.2 + float3(5.0,0.0,2.0))) / (v + 0.03);
warp_34=`
warp_35=`        // hit flag
warp_36=`        float hit = step(v, 2e-4);
warp_37=`
warp_38=`        // compute wall normal (branchless)
warp_39=`        float3 cell = p - 2.0 * floor(p/2.0);
warp_40=`        float3 normal = normalize( cell - 1.0 );
warp_41=`
warp_42=`        // reflect ray if hit
warp_43=`        float3 rNew = reflect(-r, normal);
warp_44=`        r = normalize( r*(1.0-hit) + rNew*hit );
warp_45=`
warp_46=`        // reset or advance t
warp_47=`        t = t*(1.0-hit) + 0.0*hit;
warp_48=`        t += v * 0.3 * (1.0-hit);
warp_49=`    }
warp_50=`
warp_51=`    ret = float4( tanh(c / 1000.0), 1.0 );
warp_52=`}
