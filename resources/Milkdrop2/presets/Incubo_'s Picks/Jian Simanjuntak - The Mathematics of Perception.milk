warp_2=`//ret = 1;
warp_3=`}
comp_1=`#define ITERATIONS 15
comp_2=`#define PI 3.14159265359
comp_3=`
comp_4=`float2x2 rotate2d(float angle) {
comp_5=`  return float2x2(cos(angle), -sin(angle),
comp_6=`         sin(angle), cos(angle));
comp_7=`}
comp_8=`
comp_9=`float shape(float2 st, float size, float blur) {
comp_10=`  float2 modSt = frac(st) * 2.0 - 1.0;
comp_11=`  float dist = length(modSt);
comp_12=`  return smoothstep(size + blur, size - blur, dist);
comp_13=`}
comp_14=`
comp_15=`shader_body {
comp_16=`  float2 st = (uv * 2.0 - 1.0) * float2(texsize.x/texsize.y, 1.0);
comp_17=`  
comp_18=`  float3 color = float3(0, 0, 0);
comp_19=`  float luma = 0.5;
comp_20=`  float timeScale = time * 0.5;
comp_21=`  float2 layerSt = st * (cos(timeScale * q3) * 0.6 + 0.8);
comp_22=`  layerSt = mul(layerSt, rotate2d(cos(timeScale * 0.046 + 5.0) * PI + PI));
comp_23=`
comp_24=`  for (int i = 0; i < ITERATIONS; i++) {
comp_25=`    float2 gridSt = layerSt + float2(cos(timeScale * 0.026) * 6.0 + 6.0,
comp_26=`                                 cos(timeScale * q4) * 6.0 + 6.0);
comp_27=`    layerSt = mul(layerSt, rotate2d(-cos(timeScale * 0.0024) * PI + 1.0));
comp_28=`
comp_29=`    float depth = (float(i) + 0.5) / float(ITERATIONS);
comp_30=`    float focusDepth = depth - (cos(timeScale * q6) * 0.5 + 0.5) * 0.8 + 0.1;
comp_31=`    float blur = 0.05 + focusDepth * focusDepth * 0.4;
comp_32=`
comp_33=`    float chromaticAberration = cos(timeScale * q8) * 0.2 + 0.2;
comp_34=`    float shapeSize = 0.3 + cos(timeScale * 0.148) * 0.2;
comp_35=`    float3 shapeColor = float3(
comp_36=`      shape(gridSt - st * chromaticAberration * blur, shapeSize, blur),
comp_37=`      shape(gridSt, shapeSize, blur),
comp_38=`      shape(gridSt + st * chromaticAberration * blur, shapeSize, blur)
comp_39=`    ) * luma;
comp_40=`
comp_41=`    layerSt += st * shape(gridSt, shapeSize, 0.5) * cos(timeScale * 0.162) * 0.6;
comp_42=`    layerSt *= cos(timeScale * 0.46) * 0.05 + 1.1;
comp_43=`    color += shapeColor;
comp_44=`    luma *= 0.85;
comp_45=`  }
comp_46=`
comp_47=`  ret = float4(color, 1.0);
comp_48=`}