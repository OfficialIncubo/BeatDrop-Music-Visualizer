per_frame_41=// Jian Simanjuntak edit with per_frame //
per_frame_42=dec_med = pow (0.9, 30/fps);
per_frame_43=dec_slow = pow (0.99, 30/fps);
per_frame_44=beat = max (max (bass, mid), treb); 
per_frame_45=avg = avg*dec_slow + beat*(1-dec_slow);
per_frame_46=is_beat = above(beat, .2+avg+peak) * above (time, t0+.2);
per_frame_47=t0 = is_beat*time + (1-is_beat)*t0;
per_frame_48=peak = is_beat * beat + (1-is_beat)*peak*dec_med;
per_frame_49=index = (index + is_beat) %12;
per_frame_50=index2 = (index2 + is_beat*bnot(index))%6;
per_frame_51=
