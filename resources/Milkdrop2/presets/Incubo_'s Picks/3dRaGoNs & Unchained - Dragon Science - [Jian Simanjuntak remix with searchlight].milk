comp_7=`
comp_8=`float2 hor = float2 (texsize.z,0);
comp_9=`float2 ver = float2 (0,texsize.w);
comp_10=`float dx = GetPixel (mirrorized-hor) - GetPixel (mirrorized+hor);
comp_11=`float dy = GetPixel (mirrorized-ver) - GetPixel (mirrorized+ver);
comp_12=`float2 dz = float2 (dx,dy);
comp_13=`
comp_14=`// Searchlight mod by Jian Simanjuntak 
comp_15=`    float2 searchlight = float2(uv.x - q15, uv.y - q16);
comp_16=`    float angle = atan2(searchlight.y, searchlight.x)*10;
comp_17=`    float pulse = pow(sin(time*9 + angle)*0.5+0.5, 5);
comp_18=`    searchlight = normalize(searchlight) * pulse * dz;
comp_19=`    float3 lines = saturate(0.15*q26*pulse/length(searchlight - uv + dz));
comp_20=`
comp_21=`ret1 = 1*ret1 +  lines -.1;
comp_22=`
comp_23=`ret = 1*ret1;
comp_24=`}
