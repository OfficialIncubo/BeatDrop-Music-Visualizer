per_frame_17=q2 = vy;
per_frame_18=
per_frame_19=
per_frame_20=//DVD Logo changes the color every edge bouncing
per_frame_21=if (vx >= 1 || vy >=  1 || vx <= 0 || vy <= 0, dvd_r = .25+rand(1)*.75; dvd_g = .25+rand(1)*.75; dvd_b = .25+rand(1)*.75, 0);
per_frame_22=q3 = dvd_r; q4 = dvd_g; q5 = dvd_b;
warp_1=`sampler sampler_fc_DVD_Logo;
warp_2=`
warp_3=`shader_body
warp_4=`{
warp_5=`   float2 aspect_t = float2(1, texsize.y/texsize.x);
warp_6=`   //uv = (uv-.5)*aspect_t+.5;
warp_7=`
warp_8=`   float logo_scale = 8;
warp_9=`   float logo_asp = texsize.x/texsize.y;
warp_10=`   float logo_h = .2;
warp_11=`   float logo_w = logo_h * logo_asp;
warp_12=`   float2 logo_size = float2(logo_w, logo_h);
warp_13=`
warp_14=`   float2 uv2 = uv*logo_scale - float2(q1,1-q2) * (logo_scale-1);
warp_15=`
warp_16=`   //Copilot + Se7enSlasher - Completely hide stretched lines
warp_17=`   float edge = 0;
warp_18=`   float2 fade = smoothstep(edge, .00001, uv2) *
warp_19=`                 smoothstep(edge, .00001, 1-uv2);
warp_20=`   float mask = fade.x * fade.y;
warp_21=`
warp_22=`   ret = tex2D(sampler_fc_DVD_Logo, uv2).xyz;
warp_23=`   ret -= float3(q3, q4, q5);
warp_24=`   ret *= mask;
warp_25=`}
