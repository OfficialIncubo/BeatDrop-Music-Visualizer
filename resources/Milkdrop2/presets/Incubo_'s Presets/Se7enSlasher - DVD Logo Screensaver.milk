per_frame_1=speed = (1/fps)*0.35;
per_frame_2=
per_frame_3=//Check edge collisions, directions: -1 - inverse, 0 - forward
per_frame_4=// --- RIGHT, TOP ---
per_frame_5=dirx = if (vx >= 1, 0, dirx);
per_frame_6=diry = if (vy >= 1, 0, diry);
per_frame_7=
per_frame_8=// --- LEFT, BOTTOM ---
per_frame_9=dirx = if (vx <= 0, -1, dirx);
per_frame_10=diry = if (vy <= 0, -1, diry);
per_frame_11=
per_frame_12=// Move it overtime
per_frame_13=if (dirx, vx += speed*speedoffset1, vx -= speed*speedoffset1);
per_frame_14=if (diry, vy += speed*speedoffset2, vy -= speed*speedoffset2);
per_frame_15=
