warp_1=`// Shader by Jaenam97 https://x.com/Jaenam97
warp_2=`// https://www.shadertoy.com/view/tfGyzt
warp_3=`// Ported to MilkDrop by MilkDrop2077
warp_4=`// Audio Reactive edited by Incubo_/Se7enSlasher/DJ XcitMosphere
warp_5=`
warp_6=`// 2D rotation function
warp_7=`float2x2 rotate2D(float angle)
warp_8=`{
warp_9=`   float s = sin(angle);
warp_10=`   float c = cos(angle);
warp_11=`   return float2x2(c, -s, s, c);
warp_12=`}
warp_13=`
warp_14=`shader_body
warp_15=`{
warp_16=`    float2 r = float2(texsize.x, texsize.y);
warp_17=`    float2 FC = uv_orig * r;
warp_18=`    float4 o = float4(0.0, 0.0, 0.0, 0.0);
warp_19=`
warp_20=`    // Process all three channels in a single outer loop
warp_21=`    for(int channel = 0; channel < 3; channel++)
warp_22=`    {
warp_23=`        float Z = float(channel - 1); // -1 for red, 0 for green, 1 for blue
warp_24=`        float channelValue = 0.0;
warp_25=`        float skipFlag = 0.0;
warp_26=`
warp_27=`        for(float d = 0.0, i = 0.0; i < q10; i += 1.0)
warp_28=`        {
warp_29=`            float continueFlag = 1.0 - step(0.5, skipFlag);
warp_30=`            float3 p = float3((FC * 2.0 - r) / r.y * d, d - 8.0);
warp_31=`
warp_32=`            // Check skip condition
warp_33=`            float skipCondition = step(5.0, abs(p.x));
warp_34=`            skipFlag = max(skipFlag, skipCondition * continueFlag);
warp_35=`            float processFlag = 1.0 - step(0.5, skipFlag);
warp_36=`
warp_37=`            if(processFlag > 0.5)
warp_38=`            {
warp_39=`                // Apply rotations
warp_40=`                p.xz = mul(rotate2D(q6/3), p.xz);
warp_41=`                p.xy = mul(rotate2D(q6/2), p.xy);
warp_42=`
warp_43=`                // Grid calculations
warp_44=`                float3 g = floor(p * q1);
warp_45=`                float3 f = frac(p * q7) - 0.5;
warp_46=`
warp_47=`                // Random point selection
warp_48=`                float rand1 = frac(sin(dot(g, float3(127.0, 312.0, 75.0))) * 43758.0);
warp_49=`                float h = step(length(f), rand1 * 0.3 + 0.1);
warp_50=`
warp_51=`                // Random angle
warp_52=`                float rand2 = frac(sin(dot(g, float3(44.0, 78.0, 123.0))) * 127.0);
warp_53=`                float a = rand2 * (M_PI*2);
warp_54=`
warp_55=`                // SDF calculation for boxes
warp_56=`                float e = 1.0;
warp_57=`                float sc = q9;
warp_58=`                for(int j = 0; j < 3; j++)
warp_59=`                {
warp_60=`                    float3 g2 = abs(frac(p * sc / 2.0) * 2.0 - 1.0);
warp_61=`                    float m1 = max(g2.x, g2.y);
warp_62=`                    float m2 = max(g2.y, g2.z);
warp_63=`                    float m3 = max(g2.x, g2.z);
warp_64=`                    e = min(e, min(m1, min(m2, m3)) / sc);
warp_65=`                    sc *= 0.6;
warp_66=`                }
warp_67=`
warp_68=`                // Combined SDF
warp_69=`                float absX = abs(p.x);
warp_70=`                float absY = abs(p.y);
warp_71=`                float absZ = abs(p.z);
warp_72=`                float dotAbs = dot(float3(absX, absY, absZ), float3(0.577, 0.577, 0.577)) * q8;
warp_73=`                float c = max(max(max(absX, absY), absZ), dotAbs) - (3 + q3);
warp_74=`
warp_75=`                // Ray marching step
warp_76=`                float sinC = length(sin(c));
warp_77=`                float s = 0.01 + 0.15 * abs(max(max(c, e - 0.1), sinC - 0.3) + Z * 0.02 - i / 130.0);
warp_78=`                d += s;
warp_79=`
warp_80=`                // Smooth step
warp_81=`                float sf = smoothstep(0.02, 0.01, s);
warp_82=`
warp_83=`                // Color accumulation
warp_84=`                channelValue += 1.6 / s * (0.5 + 0.5 * sin(i * 0.3 + Z * 5.0) +
warp_85=`                                          sf * 4.0 * h * sin(a + i * 0.4 + Z * 5.0));
warp_86=`            }
warp_87=`            else
warp_88=`            {
warp_89=`                // Skip this iteration - increment distance based on skip condition
warp_90=`                d += 1.0 * skipCondition;
warp_91=`            }
warp_92=`        }
warp_93=`
warp_94=`        // Assign to appropriate channel
warp_95=`        if(channel == 0) o.r = channelValue;
warp_96=`        else if(channel == 1) o.g = channelValue;
warp_97=`        else o.b = channelValue;
warp_98=`    }
warp_99=`
warp_100=`    // Final color processing
warp_101=`    o = o * o / 1.0e7;
warp_102=`    float4 exp2o = exp(2.0 * o);
warp_103=`    o = (exp2o - 1.0) / (exp2o + 1.0);
warp_104=`
warp_105=`    ret = o;
warp_106=`}
