warp_1=`//Original code: https://x.com/XorDev/status/1981737027948232741
warp_2=`//by Xor (XorDev)
warp_3=`//Converted to MilkDrop (HLSL) by DeepSeek: https://www.deepseek.com
warp_4=`//Modified by Se7enSlasher
warp_5=`
warp_6=`shader_body
warp_7=`{
warp_8=`    float2 aspect_ts = float2(1, texsize.y/texsize.x); //Self-made texsize aspect-correcting
warp_9=`    uv = ((uv - .5) * aspect_ts) / aspect_ts.y; //Then pass it to uv calculation.
warp_10=`    float2 p = .5 * uv;
warp_11=`    p.y = -p.y; //Y uv flipping is necessary
warp_12=`    float4 o = 0;
warp_13=`    
warp_14=`    for (float i = 0; i < 6; i++)
warp_15=`    {   
warp_16=`        float2 abs_val = .1 / abs(frac(p) - .5);
warp_17=`        
warp_18=`        float4 color_add = frac(length(round(p)) / exp2(i / 4.0) - time*.5) * float4(abs_val.y, abs_val.x, abs_val.x, 1.0) / i;
warp_19=`        color_add *= (1.0 - o.a);
warp_20=`        o += color_add;
warp_21=`           
warp_22=`        // Matrix multiplication: p *= mat2(2, 2, -2, 2), transposed because in MilkDrop it's using HLSL.
warp_23=`        float2x2 rot_mat = float2x2(2.0, 2.0, -2.0, 2.0);
warp_24=`        p = mul(rot_mat, p);
warp_25=`    }
warp_26=`    
warp_27=`    //XorDev - told me to replace tanh(x) with 1-exp(-x), which it works.
warp_28=`    float4 result = o * o + dot(o, o) / 1e2; //Make this as a separate result.
warp_29=`    ret = 1-exp(-result); //Then output tonemapping without thresholding.
warp_30=`}
