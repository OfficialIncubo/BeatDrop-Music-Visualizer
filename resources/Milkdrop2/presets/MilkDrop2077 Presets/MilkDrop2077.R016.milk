per_frame_6=ob_r = 1- 0.4*abs(q1);
per_frame_7=ob_g = 1.3*abs(q2);
per_frame_8=ob_b = 0.4*abs(q1);
per_frame_9=wave_x = 1-abs(q2)-1.05;
per_frame_10=wave_y = 1-abs(q1)-0.156;
per_frame_11=wave_r = wave_r + 1.4*( 0.60*sin(0.514*time) + 0.40*sin(1.211*time) );
per_frame_12=wave_b = wave_b + 1.4*( 0.60*sin(0.714*time) + 0.40*sin(q2) );
per_frame_13=wave_g = wave_g + 1.4*( 0.60*sin(10*q1) + 0.40*sin(10*q2) );
