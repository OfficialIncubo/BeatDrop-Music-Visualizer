warp_210=`        dS = GetDist(p, a);
warp_211=`        if(dS.x < 0.0001) {breakloop = 1; }
warp_212=`        dO += dS.x*0.5;
warp_213=`    }     
warp_214=`    return float3(dO,dS);
warp_215=`}
