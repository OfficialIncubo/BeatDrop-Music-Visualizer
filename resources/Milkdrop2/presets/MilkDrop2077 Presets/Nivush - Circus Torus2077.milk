warp_148=`    float hiveTorus = q14*hexTorus;
warp_149=`    
warp_150=`    float toreturn;
warp_151=`    if(a == 1)
warp_152=`        toreturn = holesTorus;
warp_153=`    else 
warp_154=`        toreturn = hiveTorus;
warp_155=`    return toreturn;
warp_156=`}
